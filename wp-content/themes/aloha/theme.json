					"left": {
						"style": "solid",
						"width": "2px"
					},
					"radius": "0px",
					"right": {
						"style": "solid",
						"width": "2px"
					},
					"top": {
						"style": "solid",
						"width": "2px"
					},
					"width": "2px"
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--40)",
						"left": "var(--wp--preset--spacing--50)",
						"right": "var(--wp--preset--spacing--50)",
						"top": "var(--wp--preset--spacing--40)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--teachers)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--gris)",
			"text": "var(--wp--preset--color--blanctxt)"
		},
		"css": ".contain img{\nobject-fit: contain!important;\n}",
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--gris)",
					"text": "var(--wp--preset--color--blanctxt)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--teachers)"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--teachers)",
