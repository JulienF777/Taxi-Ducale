				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--teachers)",
					"fontSize": "4rem",
					"fontStyle": "normal",
					"fontWeight": "500"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--teachers)",
					"fontSize": "3rem",
					"fontStyle": "normal",
					"fontWeight": "500"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--lato)",
					"fontSize": "1.2rem",
					"fontStyle": "normal",
					"fontWeight": "500"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "1.56rem"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "1.25rem"
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--blanctxt)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--teachers)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--blanctxt)"
				}
			}
		},
		"spacing": {
			"blockGap": "0px",
			"padding": {
				"bottom": "0px",
				"left": "20px",
				"right": "20px",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--lato)",
			"fontSize": "var(--wp--preset--font-size--paragraphe)",
			"lineHeight": "1"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json"
}