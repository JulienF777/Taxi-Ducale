{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "aloha-faq-block/aloha-faq",
  "version": "0.1.0",
  "title": "Aloha FAQ",
  "category": "widgets",
  "icon": "smiley",
  "description": "FAQ Block custom made by Alowa Studio",
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "aloha-faq",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js",
  "attributes": {
    "question": {
      "type": "string",
      "source": "text",
      "selector": ".faq-question"
    },
    "answer": {
      "type": "string",
      "source": "text",
      "selector": ".faq-paragraph p"
    }
  }
}